echo " autoupdater -f                    // Forced Firmware-Update
 logread      // logread -f        // syslog 
 ping 8.8.8.8                      // google-DNS when IPv4 local 
 ip a s                            // all IPs list
 ip r s t all                      // All routing tables
 traceroute -n 10.10.16.1          // Traceroute via gateway IP
 batctl gwl                        // batman-gateways
 batctl -m bat0 tr 10.10.16.1      // Traceroute via batman-adv
 batctl o                          // batman Domain - neighbors list
 batctl tl |grep W |wc -l          // batman Clients count
 batctl if                         // baman interfaces list
 brctl show                        // all bridges list
 iwinfo                            // wlan - paramter list
 iw dev client0 station dump       // WLAN neighbors details
 nodeinfo                          // Nodes info
 v4up                              // Get ipv4, when no WiFi"
